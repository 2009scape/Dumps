function httpRequest() {
 // Create the requestHandler
 try {
  this.requestHandler=new ActiveXObject("Msxml2.XMLHTTP");
 }
 catch(e1) {
  try {
   this.requestHandler=new ActiveXObject("Microsoft.XMLHTTP");
  }
